{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "../",
    "types": ["cypress", "node"],
    "noEmit": true,
    "isolatedModules": false,
    "allowJs": true,
    "lib": ["ES2022", "DOM"],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["**/*.ts", "**/*.js"],
  "exclude": ["node_modules"]
}
